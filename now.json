{
  "version": 2,
  "name": "salaryguidereloaded",
  "public": true,
  "builds": [
    {
      "src": "preact/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/assets/$1"
    }
  ]
}