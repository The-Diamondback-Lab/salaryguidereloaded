{
  "version": 2,
  "name": "salaryguidereloaded",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static",
      "config": {
        "distDir": "build",
        "files": [
          "build"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "./assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "./sw-esm.js",
      "headers": {
        "cache-control": "Immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "./(.*)\\.(.*)\\.(js|css|json|ico)$",
      "headers": {
        "cache-control": "Immutable"
      },
      "dest": "/$1.$2.$3"
    },
    {
      "src": "./(.*)\\.(js|css|json|ico)$",
      "dest": "/$1.$2"
    },
    {
      "src": "./(.*)",
      "dest": "/index.html"
    }
  ]
}